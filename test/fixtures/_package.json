{
  "name": "gulp-awesome",
  "version": "0.0.0",
  "description": "A plugin for gulp",
  "keywords": [
    "gulpplugin"
  ],
  "repository": "githubUser/gulp-awesome",
  "author": {
    "name": "Really Paul",
    "email": "really@mail.me",
    "url": "reallreally.me"
  },
  "files": ["index.js"],
  "scripts": {
    "prepublish": "gulp coffee --require coffee-script/register",
    "test": "coffeelint gulpfile.coffee index.coffee test -f ./coffeelint.json && istanbul test _mocha --report lcovonly -- ./test/*.coffee --require coffee-script/register --reporter spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "gulp-util": "^3.0.0",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "coffee-script": "^1.7.1",
    "coffeelint": "^1.4.0",
    "coveralls": "^2.8.0",
    "del": "^2.0.0",
    "gulp": "^3.5.2",
    "gulp-coffee": "^2.1.2",
    "istanbul": "^0.3.18",
    "mocha": "^2.2.5",
    "should": "^7.1.0"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.3.7"
  },
  "license": "MIT"
}
